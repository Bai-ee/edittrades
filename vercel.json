{
  "version": 2,
  "builds": [
    {
      "src": "public/**",
      "use": "@vercel/static"
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/analyze-compact/(.*)",
      "dest": "/api/analyze-compact.js"
    },
    {
      "src": "/api/analyze/(.*)",
      "dest": "/api/analyze.js"
    },
    {
      "src": "/api/indicators/(.*)",
      "dest": "/api/indicators.js"
    },
    {
      "src": "/api/scan",
      "dest": "/api/scan.js"
    },
    {
      "src": "/api/agent-review",
      "dest": "/api/agent-review.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
